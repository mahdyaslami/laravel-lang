{
    "name": "mahdiaslami/laravel-lang",
    "description": "Persian translations for Laravel lang directory.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MahdiAslami\\Laravel\\Lang\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Mahdi Aslami Khavari",
            "email": "mahdyaslami@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "MahdiAslami\\Laravel\\Lang\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^7.11.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    }
}
